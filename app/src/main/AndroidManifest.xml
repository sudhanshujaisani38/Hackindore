<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.myapplication1">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
<uses-permission android:name="android.permission.SEND_SMS"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/student"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TeacherLoginActivity">

        </activity>
        <activity android:name=".StudentLoginActivity" />
        <activity android:name=".UploadAttendanceActivity">
        </activity>
        <activity android:name=".UploadTimetableActivity" />
        <activity android:name=".TeacherProfileActivity" />
        <activity android:name=".TeacherViewActivity" />
        <activity android:name=".SelectClassActivity" />
        <activity android:name=".TimeTableActivity"></activity>
        <activity android:name=".ViewAttendanceActivity" />
        <activity android:name=".FeedbackSubmissionActivity" />
        <activity android:name=".StudentProfileActivity" />
        <activity android:name=".FeedbacksVisualizationApp"></activity>

    </application>


</manifest>